#THOMAS'S PRIME ALGORITHM

z=1
a=0
x=int(input("Enter a positive whole number"))
y=x+1
for y in range(1, y, 1):
    if(x==1):
        print(str(x)+" is not prime")
        quit()
    elif(x==2):   
        print(str(x)+" prime")
        quit()
    elif(x%2==0 and x!=2):
        print(str(x)+" is not prime")
        quit()
    elif(x%z==0):
        a=a+1 
    z=z+1
if(a<=2):
    print(str(x)+" is prime")
if(a>=3):
    print(str(x)+" is not prime")    
